if (board[i, j] != 0) {
cat(board[i, j], " ", sep = "")
} else {
cat(". ", sep = "")
}
if (j %% 3 == 0 && j < 9) cat("| ")
}
cat("\n")
if (i %% 3 == 0 && i < 9) cat("------+-------+------\n")
}
}
