library(ggplot2)
df1 <- 5  # degrees of freedom for the numerator
df2 <- 10 # degrees of freedom for the denominator
x <- seq(0, 5, length.out = 100)  # sequence of x values
y <- df(x, df1, df2)  # F distribution values
f_dist <- data.frame(x = x, y = y)
ggplot(f_dist, aes(x = x, y = y)) +
geom_line(color = "blue") +
labs(title = "F Distribution",
x = "F value",
y = "Density") +
theme_minimal()
